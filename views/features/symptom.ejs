<% layout('/layouts/boilerplate') -%>

  <link rel="stylesheet" href="/css/symptom.css">

  <body>



    <div class=" solution-card-container row row-cols-lg-6 row-col-md-4 row-col-sm-2">
      <% for(let symptom of allSymptoms) { %>
        <a class="symptom-title-link" href="/symptoms/<%=symptom._id %>">
          <div class="card symptom-card">
            <div class="symptom-img-box"><img src="<%= symptom.image %>" class="card-img-top" alt="symptom-image"></div>
            
            <div class="card-body">
              <p class="card-text">
                <%= symptom.symptom %>
              </p>
            </div>
          </div>
        </a>
        <% } %>
    </div>


  </body>